{
  "manifest_version": 3,
  "name": "Tradr Extension",
  "version": "1.0",
  "description": "Scheduled URL visitor with JS execution",
  "permissions": [
    "alarms",
    "storage",
    "scripting",
    "system.display",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Tradr"
  }
}